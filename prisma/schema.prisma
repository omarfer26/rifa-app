generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model registro {
  id       Int     @id(map: "Registro_pkey") @default(autoincrement())
  number   String  @unique(map: "Registro_number_key")
  nombre   String
  telefono String
  pago     Boolean @default(false)
}
